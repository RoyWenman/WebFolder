}


table.contain_table td { 
    height: 25px;
}

 
table.pl_class {
    width: 100%;
    clear: both;
}
 
table.pl_class td,
table.pl_class th {
    padding: 5px;
    border: 1px solid #fff;
}

/* Table header cells */
table.pl_class thead th {

  height:30px;
  vertical-align: middle;
  border-top-style: solid;
  border-bottom-style: solid;
  border-width:1px;
  /*border-color:#187E89;*/
  padding:5px;
  text-align: center;
  vertical-align: top;

  border:1px solid #E6E6F0;

  font-size: 14px;
  background: #66A3FF;
  border:1px solid #a1a1a1;
  color: #FFFFFF !important;
  text-shadow: 1px 1px 7px rgba(0, 0, 0, 0.7);

}


table.pl_class thead th a{
  color: #FFFFFF !important;
  font-weight: normal !important;
  font-size: 14px !important;;
}




