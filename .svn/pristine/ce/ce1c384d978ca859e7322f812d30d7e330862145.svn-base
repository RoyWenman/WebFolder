/*
 * DataTables framework
 */
div.dataTables_length {
    float: left;
    background-color: transparent;
}
 
div.dataTables_filter {
    float: right;
    background-color: transparent;
}
 
div.dataTables_info {
    float: left;
    background-color: transparent;
}
 
div.dataTables_paginate {
    float: right;
    background-color: transparent;
}
 
div.dataTables_length,
div.dataTables_filter,
div.dataTables_paginate,
div.dataTables_info {
    padding: 6px;
}



/*
 * Table styles
 */
 table.pl_class
{
  background-color:#777777;
  border: 1px solid #e3e3e3;
  /*font-family:Tahoma, Geneva, sans-serif;*/
  font-family: 'Open+Sans' sans-serif;
  padding:5px;
  /*box-shadow:1px 1px 9px 1px #474747;*/
  border-radius:5px;

}

 






 
 /* Self clearing - */
div.dataTables_wrapper:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}


html[xmlns] .dataTables_wrapper { display: block; }
* html .dataTables_wrapper { height: 1%; }


/*
 * Table styles
 */
 table.contain_table {
    width: 100%;
    text-align: center;
    position: relative;
    background-color: #a9a9a9;
    /*color: #ffffff;*/
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
	 height: 30px;
}
 
table.contain_table thead {
    background: transparent;	 
    height: 50px;
}

/* Nav button area*/
table.contain_table form {
    text-align: left;
    /*color: #ffffff;*/
    background-color: #a9a9a9;
}


/* HEADER BAR actually */
table.contain_table thead th {

	 height: 50px;
	 /*font-family:Tahoma, Geneva, sans-serif;*/
   font-family: 'Open+Sans' sans-serif;
