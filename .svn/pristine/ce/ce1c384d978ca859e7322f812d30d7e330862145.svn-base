/*
 * Page setup styles
 */
body {

	/*font: 80%/1.45em Arial, Verdana, Helvetica, sans-serif; /* ??? */*/
  font-family: 'Open+Sans' sans-serif;
	margin: 0;
	padding: 0;
	color: #111;
	background-color: #fff;
}

#container {
	margin: 0 auto;
	width: 100%;
	border-left: none;
   border-right: none;
   border-bottom: none;
   border-top: none;
   padding-top: 9px;
   padding-bottom: 5px;
   padding-left: 5px;
   padding-right: 5px;
   /*box-shadow: 30px 30px 30px #444;*/
  /* font-family:Tahoma, Geneva, sans-serif;*/
   font-family: 'Open+Sans' sans-serif;
   font-size: 0.8em;
}




.list_nav{
  /*text-align: right;*/
  width: 99%;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  margin:0;
  padding: 3px;
  border:1px solid #E6E6F0;

  font-size: 14px;
  /*background: #66A3FF;*/
  /*border:1px solid #a1a1a1;*/
  background: rgb(210, 210, 210);
  border:1px solid rgb(150, 150, 150);
  color: rgb(80, 80, 80);
  /*text-shadow: 1px 1px 7px rgba(0, 0, 0, 0.7);*/

}



