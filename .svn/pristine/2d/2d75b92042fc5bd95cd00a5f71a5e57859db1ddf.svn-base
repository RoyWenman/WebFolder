table.pretty {
    width: 100%;
    clear: both;
}
 
table.pretty td,
table.pretty th {
    padding: 5px;
    border: 1px solid #fff;
}

/* Header cells */
table.pretty thead th {
    text-align: center;
    background: #d3d3d3;
}





/* Body cells */
table.pretty tbody th {
    text-align: left;
    background: #dcdcdc;
    border: silver;
}
     
table.pretty tbody td {
    text-align: center;
   /* background: #d5eaf0; */
   background: white;
   border-bottom: 1px solid #d5d5d5;
   border-top: 1px solid #d5d5d5;
}
         
table.pretty tbody tr.odd td {
    /* background: #bcd9e1; */
    background: #fffff0;
    border-bottom: 1px solid #d5d5d5;
    border-top: 1px solid #d5d5d5;
}






/* Footer cells */ 
table.pretty tfoot th {
    background: #b0cc7f;
    text-align: left;
}
     
table.pretty tfoot td {
    background: #d7e1c5;
    text-align: center;
    font-weight: bold;
}

div.dataTables_wrapper {
    background-color: silver;
}


table.pretty thead th.sorting_asc {
    background: #dedede url('images/sort_asc.png') no-repeat right center;
}
 
table.pretty thead th.sorting_desc {
    background: #dedede url('images/sort_desc.png') no-repeat right center;
}
 
table.pretty thead th.sorting {
    background: #dedede url('images/sort_both.png') no-repeat right center;
}



a.paginate_button,
a.paginate_active {
    display: inline-block;
    background-color: #d5d5d5;
    padding: 2px 6px;
    margin-left: 2px;
    cursor: pointer;
    *cursor: hand;
}
 
a.paginate_active {
    background-color: transparent;
    border: 1px solid black;
}
 
a.paginate_button_disabled {
    color: #3d6672;
}
.paging_full_numbers a:active {
    outline: none
}
.paging_full_numbers a:hover {
    text-decoration: none;
}
 
div.dataTables_paginate span>a {
    width: 15px;
    text-align: center;
}
 
div.dataTables_info {
    padding: 9px 6px 6px 6px;
}





