				text-shadow: 2px 2px 6px rgba(0, 0, 0, 1);
				height: 20px;



			}
			.CO_Table_border{
				border-bottom: 1px solid #66A3FF;
			}





			.PA_Table{
				margin-top: 20px;	
			}



			.CO_TableRow td,
			.selected_table{
				vertical-align: top;
			}

			.selected_table table{
				margin-top: 0;
				margin-left: 20px;
				width:670px;
				vertical-align: top;
				text-align: center;
				height:auto;
				overflow-y: auto;
				/*overflow-x: none;*//*Horizontal*/
			}

			.selected_table table th,
			.selected_table table td{
				border: 1px solid #ccc;
			}

			.selected_table table tr th{
				/*background: #66A3FF url("../img/glossyback2.gif") repeat-x left;*/
				background: #66A3FF;
				border:1px solid #a1a1a1;
				text-shadow:rgba(0, 0, 0, 0.8) 1px 1px 3px;
			  	color: #FFF;
			  	height:18px;
			  	/*height:28px;*/
			}


			.selected_table table tbody tr td{
				border: 1px solid #ccc;
				max-height:30px;
				vertical-align: top;
				background: #fff;
				color: #444;
			}

			.selected_tr_height{
				height:30px;
				vertical-align: top;
			}

			.selected_tr_height button{
				height:30px;
				vertical-align: top;
			}


			.CO_TableCell{
				width: 100%;
				overflow-y: auto;
				overflow-x: none;/*Horizontal*/
			}

			.selected_table{
				width: 100%;
			}
 

