			/*Basic reset*/
			#reset {
				margin: 0; 
				padding: 0;
				font-family: 'Open+Sans' sans-serif;
				font-weight: normal;
			}


			#accordian {
				background: #F9F9F9;
				width: 270px;
				border:none;
				margin: 0;
				padding:0;
				overflow-y: auto;
				overflow-x: none;/*Horizontal*/
				padding-left: 1px;
			}


			/*heading styles*/
			#accordian h3 {
				font-size: 14px;
				line-height: 18px;
				color:#fff;
				/*border-top: 1px solid #a6a6a6;*/  
				background: #f0f0f0; 
				padding: 2px 2px; 
				-webkit-border-radius: 3px 3px 0 0; 
				-moz-border-radius: 3px 3px 0 0;  
				border-radius: 3px 3px 0 0;  
				-webkit-box-shadow: 0 1px 0 rgba(210,210,210,0.6); 
				-moz-box-shadow: 0 1px 0 rgba(210,210,210,0.6) ;
				box-shadow: 0 1px 0 rgba(210,210,210,0.6) ;
				text-decoration: none; 
				vertical-align: middle;


				/*padding: 5px 20px;*/
				position: relative;
				z-index: 20;
				display: block;
				/*height: 30px;*/
				cursor: pointer;
				color: #777;
				text-shadow: 1px 1px 1px rgba(255,255,255,0.8);
				/*line-height: 33px;*/
				/*font-size: 19px;*/
				background: #ffffff;
				background: -moz-linear-gradient(top, #ffffff 1%, #eaeaea 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#ffffff), color-stop(100%,#eaeaea));
				background: -webkit-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: -o-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: -ms-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#eaeaea',GradientType=0 );
				box-shadow: 
					0px 0px 0px 1px rgba(155,155,155,0.3), 
					1px 0px 0px 0px rgba(255,255,255,0.9) inset, 
					0px 2px 2px rgba(0,0,0,0.1);

			}





			.MediAccordian{
				background: #ffffff;
				background: -moz-linear-gradient(top, #ffffff 1%, #eaeaea 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#ffffff), color-stop(100%,#eaeaea));
				background: -webkit-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: -o-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: -ms-linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				background: linear-gradient(top, #ffffff 1%,#eaeaea 100%);
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#eaeaea',GradientType=0 );
				box-shadow: 
					0px 0px 0px 1px rgba(155,155,155,0.3), 
					1px 0px 0px 0px rgba(255,255,255,0.9) inset, 
					0px 2px 2px rgba(0,0,0,0.1);

				border:none !important;
			}










			/*heading hover effect*/
			#accordian h3:hover {
				background-color: #fffacd; 
				/*background: #f0f0f0;
			    background: -webkit-gradient(linear, left top, left bottom, from(#cfcfcf), to(#f6f6f6)); 
				background: -moz-linear-gradient(top, #cfcfcf, #f6f6f6);*/
			}

			/*iconfont styles*/
			#accordian h3 span{
				font-size: 16px;
			}

			#accordian h3 span a{
				font-weight: normal;
			}



			/*list items*/
			#accordian li {
				/*list-style-type: none;*/
				display: list-item;
			}


			/*links*/
			#accordian ul ul li a,
			#accordian ul ul li label {
				font-size: 11px;
				line-height: auto;
				display: block;
				/*transition for smooth hover animation*/
				transition: all 0.15s;
			}

			.grp_header{
				/*color:#fff;*/
				background: transparent;
			}


			.grp_list a{
				font-size: 12px !important;
			} 

			.grp_list h3{
				display:block !important;
			} 


			/*hover effect on links*/
			.grp_list h3,
			.grp_list h3 span,
			.grp_list h3 span a{
				width: 98%;
				/*color:#fff;*/
				background: transparent;
			}


			.grp_list h3:hover,
			.grp_list h3 span:hover,
			.grp_list h3 span a:hover{
				width: 98%;
				/*color:#fff;*/
				background: transparent;	
			}



			/*Lets hide the non active LIs by default*/
			#accordian ul ul {
				display: none;
			}
			#accordian li.active ul {
				display: block;
			}



			.sub-menu li input[type=radio] {
				position: absolute;
				opacity: 0;
			}


			.sub-menu a {
				text-decoration: none;
				border:none;
			}


			.sub-menu{
				margin-left: -20px;
				margin-top: 0px;
				margin-bottom: 0px;
			}


			.grp_list a{
				margin-left: 10px;
			}

			.sub-menu label{
				margin-left: 20px;
			}


			.sub-menu li{
				display: list-item;
				background-color: transparent;
				background-image: none !important;
				margin-left: -20px;
				margin-top: 0px;
				margin-bottom: 0px;
			}

			.sub-menu li h3{
				margin-left: 0px;
				margin-top: 1px;
				margin-bottom: 1px;
			}

			.sub-menu li h3 a{
				color: #1f1f1f !important;
				font: normal 12px;
			}


			.sub_item{
				/*height:20px;*/
				padding-top: 2px;
				padding-bottom: 4px;
				border-left: 1px solid #ccc;
				border-right: 1px solid #ccc;
			}

			.sub_item:hover{
				color:#000 !important;
				/*background: #ddd !important;*/
				background: #fffacd !important;;
				border-bottom: 1px solid #ccc;
			}				

			.sub_item{
				border-bottom: 1px solid #ccc;
				color:#222 !important;
				background: #fff !important;
			}	



			/*#######    selected items table    #############*/
			.CO_Table{
				margin-top: 0;
			}

			.CO_Table_h3{
				margin-top: 0;
				padding-top: 2px;
				font-family: 'Open+Sans' sans-serif;
				font-weight: normal;
				font-size: 1em !important;
				text-align: center;
				background: #66A3FF;
				color: #FFFFFF;
